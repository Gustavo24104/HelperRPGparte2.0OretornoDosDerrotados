            <Image x:Name="image3" Height="9" Width="9" Source="Data/MenusVermelho.png"/>
        </Button>
        <Button x:Name="btnDivMoney" Margin="283,49,0,0" Background="#FF404040" Click="btnDivMoney_Click"  RenderTransformOrigin="0.5,0.5" Height="22" VerticalAlignment="Top" HorizontalAlignment="Left" Width="19">
