                </ToolTip>
            </Button.ToolTip>
            <Image x:Name="image1" Height="9" Width="9" Source="Data/MenusVermelho.png"/>
        </Button>
        <TextBlock x:Name="txtBlockMoney" Margin="223,1,0,0" TextWrapping="Wrap" Text="Dinheiro: 0" Foreground="#FF939393" VerticalAlignment="Top" HorizontalAlignment="Left" Width="166" TextAlignment="Center" FontSize="16"/>
