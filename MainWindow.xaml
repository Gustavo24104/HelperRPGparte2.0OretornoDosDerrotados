            <Button.ToolTip>
                <ToolTip>
                    Adicionar um novo espaço para item
                </ToolTip>
            </Button.ToolTip>
            <Image x:Name="image" HorizontalAlignment="Left" Height="12" VerticalAlignment="Top" Width="12" Source="Data/MaisVerde.png"/>
